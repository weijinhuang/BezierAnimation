    <declare-styleable name="WaveProgress">
        <attr name="wave_progress_speed" format="dimension" />
        <attr name="wave_progress_stroke" format="boolean" />
        <attr name="wave_progress_line_width" format="dimension" />
        <attr name="wave_progress_amplitude" format="dimension" />
        <attr name="wave_progress_wave_color" format="color" />
    </declare-styleable>
</resources>
