<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BezierWaveView">
        <attr name="speed" format="dimension" />
        <attr name="stroke" format="boolean" />
        <attr name="line_width" format="dimension" />
        <attr name="amplitude" format="dimension" />
        <attr name="wave_color" format="color" />
